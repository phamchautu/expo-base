{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@src/*": [
        "../src/*"
      ],
      "@assets/*": [
        "../assets/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@overlay/*": [
        "../packages/overlay/*"
      ],
      "@packages/*": [
        "../packages/*"
      ],
    }
  }
}